<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <!-- Bootstrap -->
    <link href="bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="bootstrap/css/bootstrap-dialog.css" rel="stylesheet">
    <link href="css/highlight/atom-one-dark.css" rel="stylesheet">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
    {% block body %}{% endblock %}
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="bootstrap/js/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="bootstrap/js/bootstrap.min.js"></script>
    <script src="bootstrap/js/bootstrap-dialog.js"></script>
    <script src="js/lib/pixi.min.js"></script>
    <script src="js/lib/pixi.colorfill.js"></script>
    <script src="js/lib/pixi.filters.js"></script>
    <script src="js/lib/seedrandom.js"></script>
    <script src="js/lib/jquery.mousewheel.js"></script>
    <script src="js/lib/pako_deflate.min.js"></script>
    <script src="js/lib/pako_inflate.min.js"></script>
    <script src="js/lib/highlight.pack.js"></script>
    {% block javascripts %}{% endblock %}
</body>
</html>
