{
  "name": "trakos/fbpviewer",
  "license": "MIT",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-zlib": "*",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.11",
    "symfony/doctrine-bridge": "^6",
    "symfony/doctrine-messenger": "^6.0",
    "symfony/monolog-bundle": "^3.7",
    "symfony/polyfill-apcu": "^1.25",
    "symfony/console": "^6",
    "symfony/debug-bundle": "^6",
    "symfony/dotenv": "^6",
    "symfony/form": "^6",
    "symfony/framework-bundle": "^6",
    "symfony/routing": "^6",
    "symfony/runtime": "^6",
    "symfony/security-bundle": "^6",
    "symfony/twig-bundle": "^6",
    "symfony/validator": "^6",
    "symfony/web-profiler-bundle": "^6",
    "symfony/webpack-encore-bundle": "^1",
    "symfony/yaml": "^6",
    "twig/twig": "^3"
  },
  "require-dev": {
    "symfony/browser-kit": "^6",
    "symfony/phpunit-bridge": "^6"
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
